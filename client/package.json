{
  "name": "weather-winton",
  "description": "Friendly Weather: South London",
  "version": "0.2.1",
  "private": true,
  "author": {
    "name": "Louie Christie",
    "url": "https://www.louiechristie.com/"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "babel-plugin-styled-components": "^1.10.6",
    "dayjs": "^1.8.18",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '**/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "markdownlint --fix"
    ],
    "*.{js,jsx,ts,tsx,css,sass,less,graphql,yml,yaml,scss,vue}": [
      "eslint --fix",
      "prettier --write",
      "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests"
    ],
    "*.*": [
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "markdownlint-cli": "^0.21.0",
    "prettier": "^1.19.1"
  },
  "proxy": "http://localhost:5000"
}

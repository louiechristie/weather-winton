{
  "name": "weather-winton",
  "title": "Weather Winton",
  "description": "Weather Winton is an experiment in friendly weather forecasting.",
  "version": "0.28.4",
  "private": true,
  "author": {
    "name": "Louie Christie",
    "url": "https://www.louiechristie.com/"
  },
  "main": "index.js",
  "scripts": {
    "start": "yarn run build && yarn run serve",
    "dev": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch -t test",
    "test:debug": "node --experimental-vm-modules node_modules/jest/bin/jest.js -t 'test all after now are 18 degrees'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.mjs --ignore-path .gitignore . && stylelint --ignore-path .gitignore '**/*.css'",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.mjs --ignore-path .gitignore . --fix && stylelint --ignore-path .gitignore '**/*.css' --fix"
  },
  "resolutions": {
    "json5": "^2.2.3"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.18",
    "axios": "^1.7.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.11",
    "gatsby": "^5.13.5",
    "gatsby-cli": "^5.13.3",
    "gatsby-link": "^5.13.1",
    "gatsby-plugin-gatsby-cloud": "^5.13.1",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-twitter": "^5.13.1",
    "gatsby-react-router-scroll": "^6.13.1",
    "gatsby-script": "^2.13.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.13.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-universe": "^12.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "15.2.4",
    "prettier": "3.2.5",
    "react-test-renderer": "^18.3.1",
    "simple-git-hooks": "2.11.1",
    "stylelint": "16.5.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "36.0.0",
    "stylelint-prettier": "5.0.0",
    "typescript": "^5.4.5"
